{
  "name": "babajka-markup",
  "version": "1.1.4",
  "description": "Markups",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "npm run clean && cross-env PORT=3001 npm run watch",
    "start:legacy": "cross-env BABAJKA_LEGACY=true npm start",
    "build": "npm run clean && gulp build",
    "build:legacy": "cross-env BABAJKA_LEGACY=true gulp build",
    "watch": "gulp watch",
    "lint": "gulp lint && npm run eslint .",
    "deploy": "sh bin/deploy.sh",
    "prerelease": "npm version patch",
    "postrelease": "git push && git push --tags",
    "release": "npm publish",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once",
    "eslint": "eslint --ignore-path .gitignore --fix",
    "prettier": "prettier --write",
    "prettier-stylelint": "prettier-stylelint --write",
    "gulp": "gulp",
    "storybook": "start-storybook -p 6006 -s static",
    "storybook:build": "build-storybook -o dist/storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/babajka/babajka-markup.git"
  },
  "author": "babajka team <github.com/babajka>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/babajka/babajka-markup/issues"
  },
  "homepage": "https://github.com/babajka/babajka-markup#readme",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.7.2",
    "bulma": "^0.6.2",
    "bulma-badge": "^2.0.0",
    "font-awesome": "^4.7.0",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@commitlint/cli": "^7.2.0",
    "@commitlint/config-conventional": "^7.1.2",
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/git": "^7.0.4",
    "@storybook/addon-knobs": "^4.1.4",
    "@storybook/addon-viewport": "^4.1.6",
    "@storybook/html": "^4.1.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "cors": "^2.8.4",
    "cross-env": "^5.1.4",
    "css-loader": "^2.0.2",
    "ejs-webpack-loader": "^2.2.2",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.1",
    "fancy-log": "^1.3.2",
    "file-loader": "^3.0.1",
    "gh-pages": "^1.2.0",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-clean-css": "^3.9.4",
    "gulp-concat-css": "^3.1.0",
    "gulp-connect": "^5.7.0",
    "gulp-ejs": "^3.1.3",
    "gulp-htmlhint": "^2.2.1",
    "gulp-plumber": "^1.2.0",
